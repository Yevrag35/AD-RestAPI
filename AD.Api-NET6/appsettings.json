{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Settings": {
        "SearchDefaults": {
            "Computer": {
                "Size": 0,
                "Properties": [
                    "distinguishedName",
                    "cn",
                    "operatingSystem"
                ]
            },
            "Generic": {
                "Size": 10,
                "Properties": [
                    "distinguishedName",
                    "name",
                    "objectClass"
                ]
            },
            "Group": {
                "IncludeMembers": false,
                "Size": 5,
                "Properties": []
                //"Properties": [
                //    "name",
                //    "displayName",
                //    "groupType",
                //    "samAccountName"
                //]
            },
            "User": {
                "Size": 20,
                "Properties": [
                    "displayName",
                    "mail",
                    "name",
                    "samAccountName",
                    "userAccountControl",
                    "userPrincipalName"
                ]
            }
        }
    },
    "Domains": {
        "yevrag35.com": {
            "IsDefault": true,
            "DistinguishedName": "DC=yevrag35,DC=com",
            "StaticDomainController": "",
            "UseGlobalCatalog": false,
            "UseSSL": true
        }
    }
}
