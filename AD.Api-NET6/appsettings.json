{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Settings": {
        "Search": {
            "DefaultProperties": [
                "distinguishedName",
                "name",
                "objectClass",
                "samAccountName",
                "userPrincipalName"
            ]
        }
    },
    "Domains": {
        "contoso.com": {
            "IsDefault": true,
            "DistinguishedName": "DC=contonso,DC=com",
            "StaticDomainController": "StaticDC.contoso.com"
            "UseGlobalCatalog": false,
            "UseSSL": true
        }
    }
}
